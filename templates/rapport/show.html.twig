{% extends 'base.html.twig' %}

{% block title %}Rapport{% endblock %}

{% block body %}
    <h1>Rapport</h1>

    <table id="example" class="display" style="width:100%">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ rapport.id }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ rapport.statut ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Usercreate</th>
                <td>{{ rapport.usercreate }}</td>
            </tr>
            <tr>
                <th>Datecreate</th>
                <td>{{ rapport.datecreate ? rapport.datecreate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Userdelete</th>
                <td>{{ rapport.userdelete }}</td>
            </tr>
            <tr>
                <th>Datedelete</th>
                <td>{{ rapport.datedelete ? rapport.datedelete|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Userupdate</th>
                <td>{{ rapport.userupdate }}</td>
            </tr>
            <tr>
                <th>Dateupdate</th>
                <td>{{ rapport.dateupdate ? rapport.dateupdate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Rapport</th>
                <td>{{ rapport.rapport }}</td>
            </tr>
            <tr>
                <th>Mntvente</th>
                <td>{{ rapport.mntvente }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_rapport_index') }}">back to list</a>

    <a href="{{ path('app_rapport_edit', {'id': rapport.id}) }}">edit</a>

    {{ include('rapport/_delete_form.html.twig') }}
{% endblock %}
